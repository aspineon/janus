[{
    "name" : "exampleFirst",
    "active" : true,
    "proxy" : {
        "preserve_host" : false,
        "listen_path" : "/exampleFirst/*",
        "upstreams" : {
            "balancing": "roundrobin",
            "targets": [
                {"target": "http://localhost:9089/hello-world"}
            ]
        },
        "strip_path" : false,
        "append_path" : false,
        "methods" : ["GET"]
    },
    "plugins": [
        {
            "name": "rate_limit",
            "enabled": true,
            "config": {
                "limit": "5-M",
                "policy": "local"
            }
        }
    ],
    "circuit_breaker":{
      "timeout": 1,
      "max_concurrent_requests": 2,
      "request_volume_threshold": 3,
      "sleep_window": 4,
      "error_percent_threshold": 5
    },
    "health_check": {
        "url": "http://localhost:9089/status",
        "timeout": 3
    }
},
